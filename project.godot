config_version=5

[application]

config/name="Labyrinth Adventure"
run/main_scene="res://scenes/game_modes/test_stage.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://assets/Game Icon.png"

[audio]

buses/default_bus_layout="res://assets/Bus_Layout.tres"

[autoload]

GameManager="*res://globals/GameManager.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Knight Stone": "res://addons/dialogic/characters and styles/Knight Stone.dch",
"Rogue Stone": "res://addons/dialogic/characters and styles/Rogue Stone.dch",
"Stones/wizard_stone_character": "res://assets/Images/Stones/wizard_stone_character.dch",
"wizard_stone_character": "res://addons/dialogic/characters and styles/wizard_stone_character.dch"
}
directories/dtl_directory={
"Knight Stone": "res://addons/dialogic/characters and styles/Knight Stone.dtl",
"Rogue Stone": "res://addons/dialogic/characters and styles/Rogue Stone.dtl",
"Wizard Stone": "res://addons/dialogic/characters and styles/Wizard Stone.dtl"
}

[display]

window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/scene_manager/plugin.cfg")

[global_group]

attack=""
hurtboxes=""
player=""
scanner=""
UIHUD=""
enemy=""

